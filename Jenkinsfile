pipeline {
    agent any  

    stages {
        stage('Checkout Code') { // Fetches code from source control
            
        }
        stage('Install Dependencies') { // Sets up the environment
            
        }
        stage('Build & Test') { // Combines building and testing
            

        }
   

    post {
        always {
            cleanWs()  // Clean workspace after each build
        }
    }
}